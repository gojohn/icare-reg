!!!
%html(ng-app="icare")
  %head
    %title 
      Lions
    = stylesheet_link_tag    "application"
    = javascript_include_tag "jquery", "lib/angularjs/1.1.3/angular.min.js", "lib/angularjs/1.1.3/angular-resource.min.js", "lib/bootstrap.min.js", "js/app", "js/registrations"
    = csrf_meta_tags

  %body
  .outer
    = image_tag("index2015.png", :size =>"850x148")
    .navbar
      .navbar-inner
        %a.brand(href="#")
          I Care 2016 Classic - New Rider Registration

  .outer(ng-controller="RegisterCtrl")
    %ng-include(src="'/partials/register'")
